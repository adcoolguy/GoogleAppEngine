<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- generator:mapping -->
    <class name="com.ypg.model.entity.User" table="user">
        <id name="id" column="id">
            <generator class="sequence">
                <param name="sequence">user_id_seq</param>
            </generator>
        </id>
        <property name="name" column="name" not-null="true" type="string" length="0" />
        <property name="passw" column="passw" not-null="true" type="string" length="0" />
        <property name="registration" column="registration" not-null="true" type="date" />
    </class>

</hibernate-mapping>
